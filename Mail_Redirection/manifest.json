{
  "name": "DirectMail",
  "description": "Takes to mail domain with id prefilled",
  "version": "1.2",
  "browser_action": {
      "name": "Mail_redirection",
      "default_icon" : "icon2.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Jquery.js","myScript.js"]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
],
  "manifest_version": 2
}
